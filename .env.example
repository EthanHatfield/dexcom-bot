# Discord Bot Token
# Get this from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here

# Dexcom API Credentials (OAuth)
# Get these from your Dexcom developer account at https://developer.dexcom.com
DEXCOM_CLIENT_ID=your_client_id_here
DEXCOM_CLIENT_SECRET=your_client_secret_here

# Dexcom Access Tokens (obtained via OAuth flow - run authorize_dexcom.py)
# Leave these empty initially - they will be filled after running the authorization script
DEXCOM_ACCESS_TOKEN=
DEXCOM_REFRESH_TOKEN=
